import os

# Print a dictionary.
alias pd for k in %1.keys(): print("%1.", k, "=", %1[k])

# Print instance variables.
alias pi pd %1.__dict__

# Print instance variables on self.
alias ps pi self

# Print local variables.
alias pl pd locals()

# Control execution, with listing.
alias nl n;;l  # step over
alias sl s;;l  # step into
alias cl c;;l  # continue

# Quickly walk up and down the stack.
alias uu u;;u
alias uuu u;;u;;u
alias uuuu u;;u;;u;;u
alias uuuuu u;;u;;u;;u;;u
alias dd d;;d
alias ddd d;;d;;d
alias dddd d;;d;;d;;d
alias ddddd d;;d;;d;;d;;d


alias e=editor

# Edit files with elevated privileges.
alias E=sudoedit

# Edit files piped to stdin.
alias ein=editor-stdin

# Edit program(s) in $PATH.
alias ew=editor-which

# Edit results of a locate search.
alias el=editor-locate

# Edit results of an rg search.
alias erg=editor-rg

# Edit results of an rg search via sk.
alias ergsk=editor-rg-sk

# vi:ft=sh

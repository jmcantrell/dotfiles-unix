# Set text for register(s).
alias ys='yank set'
alias ysa='yank -a set' # all registers

# Append text to register(s).
alias ya='yank add'
alias yaa='yank -a add' # all registers

# Edit register(s).
alias ye='yank edit'
alias yea='yank -a edit' # all registers

# Display text in register(s).
alias yp='yank print'
alias ypa='yank -a print' # all registers

# Delete register(s).
alias yc='yank clear'
alias yca='yank -a clear' # all registers

# List text registers with content.
alias yl='yank list'

# vi:ft=sh

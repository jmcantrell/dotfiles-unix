# create new or attach to session
alias tn='tmux new-session -AD'

# attach to or detach from session
alias ta='tmux attach-session'
alias td='tmux detach-client'

# rename window or session
alias trn='tmux rename-window'
alias trns='tmux rename-session'

# list objects
alias tls='tmux list-sessions'
alias tlc='tmux list-clients'
alias tlw='tmux list-windows'
alias tlp='tmux list-panes'
alias tlk='tmux list-keys'

# kill session or server
alias tk='tmux kill-session'
alias tko='tmux kill-server'

# vi:ft=sh

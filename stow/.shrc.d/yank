alias y="yank"
alias ya="yank -a"
alias yl="yank -l"
alias yx="yank -x"
alias yc="yank -c"

ycp() {
    yank -l | xargs -r -I{} cp -av {} "${1:-$PWD}"
}

ymv() {
    yank -l | xargs -r -I{} mv -v {} "${1:-$PWD}"
    yank -c
}

yrm() {
    yank -l | xargs -r -I{} rm -irfv {}
    yank -c
}

# vi:ft=sh
